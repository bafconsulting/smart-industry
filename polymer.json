{
  "entrypoint": "index.html",
  "shell": "src/view-index.html",
  "fragments": [
    "src/page/view-dashboard.html",
    "src/page/view-setup.html",
    "src/page/view-planning.html",
    "src/page/view-tracking.html",
    "src/page/view-404.html",
    "src/page/view-settings.html",
    "src/page/view-login.html"
  ],
  "sources": [
